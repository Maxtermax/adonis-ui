{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "transform": {
      "react": {
        "importSource": "react"
      }
    },
    "parser": {
      "syntax": "ecmascript",
      "jsx": true
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/core/*"],
      "utils/*": ["./src/utils/*"],
      "components/*": ["./src/core/components/*"],
      "ADAccordion/*": ["./src/core/components/ADAccordion/*"],
      "ADText/*": ["./src/core/components/ADText/*"],
      "ADMedia/*": ["./src/core/components/ADMedia/*"],
      "ADMedia/queries/media": ["./src/core/components/ADMedia/queries/media"],
      "ADMedia/components/Header/Header": ["./src/core/components/ADMedia/Header/Header"],
      "ADButton/*": ["./src/core/components/ADButton/*"],
      "ADCard/*": ["./src/core/components/ADCard/*"],
      "ADPanel/*": ["./src/core/components/ADPanel/*"],
      "ADSlides/*": ["./src/core/components/ADSlides/*"],
      "ADBadge/*": ["./src/core/components/ADBadge/*"],
      "ADDrawer/*": ["./src/core/components/ADDrawer/*"],
      "ADCheckbox/*": ["./src/core/components/ADCheckbox/*"],
      "ADTooltip/*": ["./src/core/components/ADTooltip/*"],
      "constants": ["./src/core/constants"],
      "theme": ["./src/core/theme/Light.js"]
    },
    "target": "es2022",
    "minify": {
      "compress": {
        "arguments": false,
        "arrows": true,
        "booleans": true,
        "booleans_as_integers": false,
        "collapse_vars": true,
        "comparisons": true,
        "computed_props": true,
        "conditionals": true,
        "dead_code": true,
        "directives": true,
        "drop_console": false,
        "drop_debugger": true,
        "evaluate": true,
        "expression": false,
        "hoist_funs": false,
        "hoist_props": true,
        "hoist_vars": false,
        "if_return": true,
        "join_vars": true,
        "keep_classnames": false,
        "keep_fargs": true,
        "keep_fnames": false,
        "keep_infinity": false,
        "loops": true,
        "negate_iife": true,
        "properties": true,
        "reduce_funcs": false,
        "reduce_vars": true,
        "side_effects": true,
        "switches": true,
        "typeofs": true,
        "unsafe": false,
        "unsafe_arrows": false,
        "unsafe_comps": false,
        "unsafe_Function": false,
        "unsafe_math": false,
        "unsafe_symbols": false,
        "unsafe_methods": false,
        "unsafe_proto": false,
        "unsafe_regexp": false,
        "unsafe_undefined": false,
        "unused": true,
        "const_to_let": true,
        "pristine_globals": true
      },
      "mangle": {
        "toplevel": false,
        "keep_classnames": false,
        "keep_fnames": false,
        "keep_private_props": false,
        "ie8": false,
        "safari10": false
      }
    }
  },
  "module": {
    "type": "es6"
  },
  "minify": true,
  "isModule": true
}
